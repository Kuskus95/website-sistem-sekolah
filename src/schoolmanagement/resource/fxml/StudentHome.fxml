<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTreeTableView?>
<?import javafx.scene.control.TreeTableColumn?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="schoolmanagement.java.controllers.StudentHomeController"
            prefWidth="600" prefHeight="400">

    <VBox spacing="10" AnchorPane.topAnchor="10" AnchorPane.leftAnchor="10" AnchorPane.rightAnchor="10" AnchorPane.bottomAnchor="10">
        <JFXTreeTableView fx:id="gradesTable" prefHeight="380" prefWidth="580">
            <columns>
                <TreeTableColumn fx:id="courseCol" text="Course" prefWidth="300"/>
                <TreeTableColumn fx:id="gradeCol" text="Grade" prefWidth="280"/>
            </columns>
        </JFXTreeTableView>
    </VBox>

</AnchorPane>
